{
  "name": "website2",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:staging": "NODE_ENV=staging vite build --outDir dist-staging",
    "build:production": "NODE_ENV=production vite build --outDir dist-production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "predeploy:staging": "npm run build:staging",
    "predeploy:production": "npm run build:production",
    "deploy": "npm run deploy:production",
    "deploy:staging": "npm run predeploy:staging && gh-pages -d dist-staging -b deployed-staging",
    "deploy:production": "npm run predeploy:production && gh-pages -d dist-production -b deployed-production",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "update-data": "tsx data-generators/all.ts"
  },
  "dependencies": {
    "@argonprotocol/apps-core": "^0.0.1",
    "@argonprotocol/mainchain": "^1.3.22",
    "@chenfengyuan/vue-countdown": "2",
    "@heroicons/vue": "^2.1.5",
    "bignumber.js": "^9.3.1",
    "chart.js": "^4.5.1",
    "d3": "^7.9.0",
    "dayjs": "^1.11.13",
    "numeral": "^2.0.6",
    "pinia": "^2.1.7",
    "reka-ui": "^2.6.1",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tailwindcss/vite": "^4.1.12",
    "@tsconfig/node20": "^20.1.4",
    "@types/d3": "^7",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@types/numeral": "^2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "gh-pages": "^6.3.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.40",
    "prettier": "^3.2.5",
    "sass": "^1.83.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.5",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  },
  "resolutions": {
    "@argonprotocol/apps-core": "portal:../apps/core"
  },
